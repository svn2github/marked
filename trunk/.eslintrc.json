{
  "extends": "standard",
  "plugins": [
    "standard"
  ],
  "rules": {
    "semi": "off",
    "space-before-function-paren": "off",
    "operator-linebreak": ["error", "before", { "overrides": { "=": "after" } }],
    "no-cond-assign": "off",
    "no-useless-escape": "off",
    "no-return-assign": "off",
    "one-var": "off"
  }
}
